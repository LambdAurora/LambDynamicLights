{
  "schema_version": 1,
  "quilt_loader": {
    "group": "dev.lambdaurora",
    "id": "lambdynlights",
    "version": "${version}",
    "metadata": {
      "name": "LambDynamicLights",
      "description": "Adds dynamic lights to the game.",
      "contributors": {
        "LambdAurora": "Owner",
        "5HT2": "Contributor"
      },
      "contact": {
        "homepage": "https://modrinth.com/mod/lambdynamiclights",
        "sources": "https://github.com/LambdAurora/LambDynamicLights.git",
        "issues": "https://github.com/LambdAurora/LambDynamicLights/issues"
      },
      "license": "MIT",
      "icon":  "assets/lambdynlights/icon.png"
    },
    "entrypoints": {
      "client_init": [
        "dev.lambdaurora.lambdynlights.LambDynamicLights::INSTANCE"
      ],
      "client_events": [
        "dev.lambdaurora.lambdynlights.LambDynamicLights::INSTANCE"
      ],
      "modmenu": [
        "dev.lambdaurora.lambdynlights.LambDynamicLightsModMenu"
      ]
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "depends": [
      {
        "id": "minecraft",
        "versions": "~1.19.4"
      },
      {
        "id": "quilt_loader",
        "versions": ">=0.17.11"
      },
      "quilt_block_extensions",
      "quilt_resource_loader",
      "fabric-renderer-api-v1",
      {
        "id": "spruceui",
        "versions": ">=4.2.0"
      },
      {
        "id": "java",
        "versions": ">=17"
      },
      {
        "id": "modmenu",
        "versions": ">=5.0.0",
        "optional": true
      }
    ],
    "breaks": [
      "optifabric"
    ]
  },
  "mixin": "lambdynlights.mixins.json",
  "access_widener": "lambdynlights.accesswidener",
  "minecraft": {
    "environment": "client"
  }
}
